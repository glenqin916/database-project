CREATE TABLE PATRON (
   fname varchar(30),
   lname varchar(30),
   library_card boolean,
   address varchar(40),
   email varchar(40),
   library varchar(30),
   primary key(email),
   foreign key(library) references LIBRARY(address)
);

CREATE TABLE LIBRARY (
   address varchar(40),
   hours time,
   primary key(address)
);

CREATE TABLE CHECKOUTS (
    due_date date,
    transaction_id integer,
    return_date date,
    cid integer,
    patron varchar,
    primary key(transaction_id),
    foreign key(cid) references CONTENT(content_id),
    foreign key(patron) references PATRON(email)
);

CREATE TABLE ORDERS (
   order_id integer,
   price decimal(15,2),
   arrival_date date,
   type varchar(10),
   cid integer,
   library varchar(40),
   primary key(order_id),
   foreign key(cid) references CONTENT(content_id),
   foreign key(library) references LIBRARYa(address)
);

CREATE TABLE CONTENT (
   content_id integer,
   location varchar(30),
   num_copies integer,
   content_type varchar(10),
   library varchar(40),
   primary key(content_id),
   foreign key(library) references LIBRARY(address)
);

CREATE TABLE PEOPLE (
   creator_id integer,
   primary key(creator_id)
);

CREATE TABLE ARTIST (
   name varchar(30),
   creator integer,
   primary key(creator),
   foreign key(creator) references PEOPLE(creator_id)
);

CREATE TABLE AUTHOR (
   name varchar(30),
   creator integer,
   primary key(creator),
   foreign key(creator) references PEOPLE(creator_id)
);

CREATE TABLE ACTOR (
   name varchar(30),
   creator integer,
   primary key(creator),
   foreign key(creator) references PEOPLE(creator_id)
);

CREATE TABLE DIRECTOR (
   name varchar(30),
   creator integer,
   primary key(creator),
   foreign key(creator) references PEOPLE(creator_id)
);

CREATE TABLE ALBUM1 (
   album_name varchar(20),
   year integer,
   genre varchar(10),
   content integer,
   primary key(content),
   foreign key(content) references CONTENT(content_id)
);

CREATE TABLE ALBUM2 (
   artist varchar(30),
   content integer,
   primary key(content),
   foreign key(artist) references ARTIST(name),
   foreign key(content) references CONTENT(content_id)
);

CREATE TABLE TRACK (
   track_name varchar(20),
   length time,
   album integer,
   primary key(album),
   foreign key(album) references CONTENT(content_id)
);

CREATE TABLE BOOK1 (
   book_title varchar(30),
   genre varchar(10),
   publisher varchar(30),
   year integer,
   chapters integer,
   pages integer,
   content integer,
   audio_format boolean,
   primary key(content),
   foreign key(content) references CONTENT(content_id)
);

CREATE TABLE BOOK2 (
   author varchar(30),
   content integer,
   primary key(content),
   foreign key(author) references AUHTOR(name),
   foreign key(content) references CONTENT(content_id)
);

CREATE TABLE MOVIE1 (
   title varchar(30),
   year integer,
   length time,
   genre varchar(10),
   content_rating char(4),
   producer varchar(30),
   director varchar(30),
   content integer,
   primary key(content),
   foreign key(director) references DIRECTOR(name),
   foreign key(content) references CONTENT(content_id)
);

CREATE TABLE HASROLEIN (
   actor varchar(30),
   movie integer,
   role_type varchar(30),
   primary key(actor, movie),
   foreign key(actor) references ACTOR(name),
   foreign key(movie) references CONTENT(content_id)
);

CREATE TABLE CONTAINS (
   order_id integer,
   cid integer,
   primary key(order_id, cid),
   foreign key(order_id) references ORDERS(order_id),
   foreign key(cid) references CONTENT(content_id)
);

CREATE TABLE SEARCHESPEOPLE (
   patron varchar(40),
   creator_id integer,
   primary key(patron, creator_id),
   foreign key(patron) references PATRON(email),
   foreign key(creator_id) references PEOPLE(creator_id)
);

CREATE TABLE SEARCHESCONTENT (
   patron varchar(40),
   content_id integer,
   primary key(patron, content_id),
   foreign key(patron) references PATRON(email),
   foreign key(content_id) references CONTENT(content_id)
);
